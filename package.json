{
  "name": "remix-full-stack-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:docker": "./.devcontainer/run-playwright.sh",
    "test:e2e:remote": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:3000/ npx -y playwright@1.53.0 test",
    "playwright:server:start": "./.devcontainer/playwright-server.sh start",
    "playwright:server:stop": "./.devcontainer/playwright-server.sh stop",
    "playwright:server:logs": "./.devcontainer/playwright-server.sh logs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "echo 'Skipping prisma generate - run manually: yarn db:generate'"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@remix-run/server-runtime": "^2.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "isbot": "^4.4.0",
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.7.0",
    "remix-auth-google": "^1.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "^1.53.0",
    "@remix-run/dev": "^2.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitest/ui": "^1.2.1",
    "autoprefixer": "^10.4.16",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22"
}
